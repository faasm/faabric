table SnapshotPushRequest {
  key:string;
  groupid:int;
  maxSize:ulong;
  contents:[ubyte];
}

table SnapshotDeleteRequest {
  key:string;
}

table SnapshotDiffChunk {
  offset:int;
  dataType:int;
  mergeOp:int;
  data:[ubyte];
}

table SnapshotDiffPushRequest {
  key:string;
  groupid:int;
  force:bool;
  chunks:[SnapshotDiffChunk];
}

table ThreadResultRequest {
  message_id:int;
  return_value:int;
}
